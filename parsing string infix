#include <iostream>
#include <cstring>
#include <vector>
using namespace std;

void Separating(string _input){
    vector<char> sprtd;
    string data;
    char cNum[] = "0123456789";
    char cSym[] = "+-/%*";
    char cSpc[] = "()";
    char chr[1];
    
    for(auto charInput : _input)
	{
        chr[0] = charInput;

        if(strspn(chr, cSym) || strspn(chr, cNum) || strspn(chr, cSpc))
		{
            sprtd.push_back(charInput);
        }
    }
    
    
    for (int j = 0; j != sprtd.size(); j++)
	{    
        chr[0] = sprtd[i];

        if((strspn(chr, cSym) || sprtd[j] == ')') && sprtd[j] != '(' && j > 0)
		{
            data += " ";
        }
        
        if(strspn(chr, cNum)){
            data += sprtd[j];
        }else{
            data += sprtd[j];


            if(j > 0){
                chr[0] = sprtd[j - 1];


                if(sprtd[i] != '(' && strspn(chr, cSym)){
                    data += "1 * ";
                }else if(sprtd[j] != ')'){
                    chr[0] = sprtd[j + 1];

                    if(!strspn(chr, cSym)){
                        data += " ";
                    }
                }
            }else if(sprtd[j + 1] == '('){
                data += "1 * ";                
            }
        }
    }
    cout << data << endl;
}

int main(){
    string input;

    getline(cin, input);
 
    
    Separating(input);
}
